{
  "name": "boparaiamrit/multitenant",
  "description": "Multitenant laravel lts with separation between tenant and system",
  "keywords": [
    "laravel",
    "multitenant",
    "multi-tenancy"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Amritpal Singh",
      "email": "boparaiamrit@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/boparaiamrit/multitenant/issues",
    "source": "https://github.com/boparaiamrit/multitenant",
    "docs": "https://boparaiamrit.readme.io"
  },
  "require": {
    "php": ">=7.0.0",
    "phpseclib/phpseclib": "^2.0",
    "doctrine/dbal": "^2.5"
  },
  "suggest": {
    "boparaiamrit/management-interface": "Provides a user interface for managing multi tenancy"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0",
    "scrutinizer/ocular": "~1.1",
    "symfony/dom-crawler": "2.8.*|3.0.*|3.1.*"
  },
  "autoload": {
    "psr-4": {
      "Boparaiamrit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hyn\\Tests\\Seeds\\": "tests/database/seeds/"
    }
  },
  "minimum-stability": "stable"
}
